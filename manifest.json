{
  "manifest_version": 2,

  "name": "re-present facebook live video",
  "description": "re-present facebook live video",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "re-present facebook live"
  },
  "permissions": [
    "tabs",
    "https://www.facebook.com/*"
  ]
}
