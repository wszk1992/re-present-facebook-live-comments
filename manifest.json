{
  "manifest_version": 2,

  "name": "test2",
  "description": "test2",
  "version": "1.0",
  "content_security_policy": "script-src 'self' https://connect.facebook.net https://graph.facebook.com; object-src 'self'",
  "background": {
    "persistent": false,
    "page": "background.html",
    "script": "background.js"
  },
    "browser_action": {
    "default_icon": "icon.png",
    "default_title": "test2",
    "default_popup": "popup.html" 
  },
  "permissions": [
    "tabs",
    "https://*/*"
  ]
}
